{
  "compilerOptions": {
    "isolatedModules": false,
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components": ["src/components"],
      "@modules": ["src/modules"],
      "@hooks": ["src/hooks"],
      "@constants": ["src/constants"],
    }
  }
}
